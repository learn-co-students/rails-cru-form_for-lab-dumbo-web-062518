<h2>Edit This Song</h2>
<%= form_for @song do |f| %>
<%= f.label :name, "Song Name: " %>
<%= f.text_field :name %><br>
<%= f.label :artist_id, "Artist: " %>
<%= f.text_field :artist_id %><br>
<%= f.label :genre_id, "Genre: " %>
<%= f.text_field :genre_id %><br>
<%= f.submit "Update Song" %>
<% end %>
