<h2>New Song</h2>
<%= form_for @song do |f| %>
<%= f.label :name, "Song: " %>
<%= f.text_field :name, placeholder: "Song Title" %><br>
<%= f.label :artist_id, "Artist: " %>
<%= f.text_field :artist_id, placeholder: "Artist" %><br>
<%= f.label :genre_id, "Genre: " %>
<%= f.text_field :genre_id, placeholder: "Genre" %><br>
<%= f.submit "Create Song" %>
<% end %>
